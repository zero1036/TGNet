@{
    ViewBag.Title = "WXPictureConfig2";
    Layout = null;
}

<!DOCTYPE html>
<html lang="zh-cn">
<head>
    <meta charset="utf-8">
    <!--IE兼容标签-->
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <!--响应式布局-->
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <!--默认使用webkit内核进行渲染-->
    <meta name="renderer" content="webkit">
    @*<title>Bootstrap 101 Template</title>*@

    <!-- Bootstrap -->
    @*<link href="~/Scripts/Bootstrap/css/bootstrap.min.css" rel="stylesheet">*@
    @*@Styles.Render("~/Content/bootstrapcss")*@
    <!-- bootstrapcss 采用百度CDN -->
    <link href="http://libs.baidu.com/bootstrap/3.0.3/css/bootstrap.min.css" rel="stylesheet">
    <!-- Dropzone -->
    @Styles.Render("~/Content/dropzonescss")

    <!-- HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
      <script src="http://cdn.bootcss.com/html5shiv/3.7.2/html5shiv.min.js"></script>
      <script src="http://cdn.bootcss.com/respond.js/1.4.2/respond.min.js"></script>
    <![endif]-->
</head>

<body ng-app="WXApp" ng-controller="PictureListCtrl">
    <!-- 採用CDN數據源，JQuery Bootstrap 由于Jquery2.0版本以上不支持IE678，改用1.9.1-->
    <!--<script src="http://libs.baidu.com/jquery/2.0.0/jquery.min.js"></script>-->
    <script src="http://libs.baidu.com/jquery/1.9.1/jquery.min.js"></script>
    <script src="http://libs.baidu.com/bootstrap/3.0.3/js/bootstrap.min.js"></script>
    <!--jquery.json-->
    @Scripts.Render("~/Scripts/jquery.json-2.3.min.js")
    <!-- Include all compiled plugins (below), or include individual files as needed -->
    @Scripts.Render("~/Bundles/angularjs")
    <!-- Dropzone -->
    @Scripts.Render("~/Bundles/dropzonejs")
    <!-- Controllers -->
    @Scripts.Render("~/Scripts/Controllers/app.js")
    @Scripts.Render("~/Scripts/Controllers/WXBaseController.js")
    @Scripts.Render("~/Scripts/Controllers/WXOrgController.js")
    @Scripts.Render("~/Scripts/Controllers/WXResController.js")
    @Scripts.Render("~/Scripts/Controllers/WXService.js")
    @Scripts.Render("~/Scripts/Controllers/WXDirective.js")
    <!-- EGCommon -->
    @Scripts.Render("~/Bundles/egcommonjs")

    <style>
        #dropZone {
            background: gray;
            border: black dashed 3px;
            width: 200px;
            padding: 50px;
            text-align: center;
            color: white;
        }
    </style>
    <div class="container-fluid">
        <div class="row" style="margin-left: 10px; margin-right: 10px">
            <div class="col-md-7">
                <wxpictures></wxpictures>
            </div>
            <div class="col-md-5">
                <div class="panel panel-default">
                    <div class="panel-body">
                        <p>选择上传图片资源</p>
                        <div class="btn-group btn-group-sm btn-group-block" data-toggle="buttons">
                            <label class="btn btn-default active">
                                <input type="checkbox" autocomplete="off" checked>
                                微信資源
                            </label>
                            <label class="btn btn-default">
                                <input type="checkbox" autocomplete="off">
                                本地資源
                            </label>
                        </div>

                        <div class="jumbotron_M">
                            <form dropzonex action="/WXResource/ImageUpLoad" method="post" enctype="multipart/form-data" class="dropzone" id="dropzoneForm">
                                <div class="fallback">
                                    <input name="file" type="file" multiple />
                                    <input type="submit" value="Upload" />
                                </div>
                            </form>
                        </div>
                    </div>
                    <div>
                        <button type="submit" id="submit-all" disabled="disabled" class="btn btn-primary btn-lg btn-block">上  传</button>
                    </div>
                    <div class="panel-footer">圖片資源上傳</div>
                </div>
            </div>

        </div>

    </div>


</body>
</html>

